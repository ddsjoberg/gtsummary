citHeader("To cite the survey package in publications use one or more of:")

citEntry(entry="misc",
   author="Thomas Lumley",
   year=2020,
   title="survey: analysis of complex survey samples",
   note="R package version 4.0",
   textVersion="T. Lumley (2020) \"survey: analysis of complex survey samples\". R package version 4.0." )



citEntry(entry="article",
  year=2004,
  author="Thomas Lumley",
  title = "Analysis of Complex Survey Samples",
  journal="Journal of Statistical Software",
  volume="9",number=1,pages="1-19",
  note="R package verson 2.2",
  textVersion="T. Lumley (2004) Analysis of complex survey samples. Journal of Statistical Software 9(1): 1-19"
)
   

citEntry(entry="book",
  year=2010,
  author="Thomas Lumley",
  title = "Complex Surveys: A Guide to Analysis Using R: A Guide to Analysis Using R",
  publisher="John Wiley and Sons",
  textVersion="T. Lumley (2010) Complex Surveys: A Guide to Analysis Using R. John Wiley and Sons."
)
   
